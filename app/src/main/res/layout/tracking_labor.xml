<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/md_20"
    android:clickable="true"
    android:focusable="true"
    android:foreground="?android:attr/selectableItemBackground"
    card_view:cardCornerRadius="10dp"
    card_view:cardElevation="@dimen/md_15"
    card_view:cardUseCompatPadding="true">



    <RelativeLayout
        android:layout_margin="@dimen/_10sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <TextView
            android:id="@+id/textjobtitile1"
            android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
            android:textSize="@dimen/_13sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_5sdp"
            android:textColor="@color/colortext"
            android:text="@string/area"/>

        <TextView
            android:id="@+id/textjobtitile"
            android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textSize="@dimen/_13sdp"
            android:singleLine="true"
            android:layout_toRightOf="@id/textjobtitile1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colortext" />


        <TextView
            android:id="@+id/textjobid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
           ></TextView>
      <TextView
            android:id="@+id/textlastaddress1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
          android:visibility="invisible"
            />
        <TextView
            android:id="@+id/textlastaddress2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"

            />
        <TextView
            android:id="@+id/textlastaddressss"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"

            />
        <TextView
            android:id="@+id/textjob_area1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textjobtitile"
            android:textColor="@color/colortext"
            android:textSize="@dimen/_13sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:text="@string/pincode1"/>
        <TextView
            android:id="@+id/textjob_area"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textjobtitile"
            android:layout_toRightOf="@id/textjob_area1"
            android:textColor="@color/colortext"
            android:singleLine="true"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textSize="@dimen/_13sdp"/>
        <TextView
            android:id="@+id/textjob_wages1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textjob_area1"
            android:textColor="@color/colortext"
            android:textSize="@dimen/_13sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:text="@string/wages"/>
        <TextView
            android:id="@+id/textjob_wages"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_below="@id/textjob_area"
            android:layout_toRightOf="@id/textjob_wages1"
            android:textColor="@color/colortext"
            android:textSize="@dimen/_13sdp"/>

        <TextView
            android:id="@+id/texttime1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textjob_wages1"
            android:layout_marginLeft="@dimen/_5sdp"
            android:textColor="@color/colortext"
             android:textSize="@dimen/_13sdp"
            android:text="@string/time"/>

        <TextView
            android:id="@+id/texttime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textjob_wages"
            android:layout_toRightOf="@id/texttime1"
            android:textColor="@color/colortext"
            android:layout_marginLeft="@dimen/_5sdp"
            android:singleLine="true"
            android:textSize="@dimen/_13sdp"/>
        <TextView
            android:id="@+id/textlabor_id1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/texttime"
            android:textColor="@color/colortext"
            android:layout_marginLeft="@dimen/_5sdp"

            android:textSize="@dimen/_13sdp"
            android:text="@string/labourname"/>
        <TextView
            android:id="@+id/textlabor_id"
            android:visibility="invisible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/textlabor_idname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/texttime"
            android:layout_marginLeft="@dimen/_5sdp"
            android:singleLine="true"
            android:layout_toRightOf="@id/textlabor_id1"
            android:textColor="@color/colortext"
            android:textSize="@dimen/_13sdp"/>
        <TextView
            android:id="@+id/textcontractor_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
         />

        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkboxtrack"
           android:layout_marginLeft="@dimen/_200sdp"
            android:layout_alignBaseline="@+id/textlabor_idname"
            android:background="@color/background"></CheckBox>



    </RelativeLayout>

</androidx.cardview.widget.CardView>